<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jsf="http://jsf.jd.com/schema/jsf"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://jsf.jd.com/schema/jsf http://jsf.jd.com/schema/jsf/jsf.xsd">

    <!-- 服务端 -->
    <jsf:server id="jsf" protocol="jsf"/>

    <bean id="strategicBusinessJsfServiceImpl"
          class="com.jd.fms.merchantmaster.jsf.strategicbusiness.impl.StrategicBusinessJsfServiceImpl"></bean>

    <jsf:provider id="strategicBusinessJsfService"
                  interface="com.jd.fms.merchantmaster.jsf.strategicbusiness.StrategicBusinessJsfService"
                  alias="STRATEGIC_BUSINESS"
                  ref="strategicBusinessJsfServiceImpl"
                  server="jsf" >
    </jsf:provider>

</beans>